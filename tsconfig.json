{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/comun": [
        "libs/comun/src"
      ],
      "@app/comun/*": [
        "libs/comun/src/*"
      ],
      "@app/seguridad": [
        "libs/seguridad/src"
      ],
      "@app/seguridad/*": [
        "libs/seguridad/src/*"
      ],
      "@lib/comun": [
        "libs/comun/src"
      ],
      "@lib/comun/*": [
        "libs/comun/src/*"
      ],
      "@lib/seguridad": [
        "libs/seguridad/src"
      ],
      "@lib/seguridad/*": [
        "libs/seguridad/src/*"
      ]
    }
  }
}